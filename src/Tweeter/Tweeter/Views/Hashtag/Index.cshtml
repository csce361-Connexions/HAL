@model IEnumerable<Tweeter.Models.Hashtag>

@{
    ViewBag.Title = "Index";
}
<div id="hashtags" class="irreplaceable">
    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.name)
            </th>
            <th></th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink("#" + item.name, "Details", new { id = item.Id })
                </td>
            </tr>
        }
    </table>
</div>